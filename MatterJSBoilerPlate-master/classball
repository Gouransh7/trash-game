class ball{
constructor(x,y){
var option{
   ' resitution':0.8;
    'friction':0.3;
    'density':1.0;
}
this.body=Bodies.rectangle(x,y,50,50,options);
this.width=50;
this.height=50;
Wrold.add(world. this.body);
}
display(){
    var pos=this.body.position;
    var angle=this.body.angle;
push()
translate(pos.x. pos.y);
rotate(angle);
rectMode(CENTER);
fill("pink");
rect(0,0,this.width,this.height);
pop();
}

};